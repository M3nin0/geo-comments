      "type": "string",
      "enum": ["A", "D"]
    },
    "comment": {
      "description": "Description for record (may contain HTML).",
      "type": "string"
    },
    "topics": {
      "$ref": "local://feedback/definitions-v1.0.0.json#/topics"
    },
    "user_id": {
      "$ref": "local://feedback/definitions-v1.0.0.json#/user"
    },
    "record_pid": {
